    "flowbite": "^1.6.5",
    "flowbite-react": "^0.4.3",
    "formik": "^2.2.9",
    "framer-motion": "^10.11.6",
    "moment": "^2.29.4",
    "naija-state-local-government": "^1.1.2",
    "next": "^13.3.0",
    "next-auth": "^4.22.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.8.0",
    "react-loading-skeleton": "^3.3.1",
    "react-modal": "^3.16.1",

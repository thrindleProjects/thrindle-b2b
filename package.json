    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@iconify/react": "^4.1.0",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.5",
